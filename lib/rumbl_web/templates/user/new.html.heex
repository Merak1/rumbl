<div>
    <%= form_for @changeset , Routes.user_path(@conn, :create), fn f ->  %> 

<div>
    <%= text_input f, :name , placeholder: "Name" , class: "test" %>
    <%= error_tag f, :name %>
</div>

<div>
    <%= text_input f, :username , placeholder: "User name" , class: "test" %>
    <%= error_tag f, :username %>
</div>

<%=  submit "Create User" %>
<% end %>
</div>